{"ast":null,"code":"import Store from \"../store\";\nexport default {\n  name: \"CalendarEvent\",\n  props: {\n    event: Object,\n    day: Object\n  },\n\n  data() {\n    return {\n      newEventTitle: \"\",\n      newEventPriority: this.event.priority\n    };\n  },\n\n  computed: {\n    priorityDisplayName() {\n      switch (this.event.priority) {\n        case 1:\n          return \"Niedrig\";\n\n        case 0:\n          return \"Mittel\";\n\n        case -1:\n          return \"Hoch\";\n      }\n\n      return \"Unbekannte Prio\";\n    },\n\n    alertColor() {\n      return \"alert-\" + this.event.color;\n    }\n\n  },\n  methods: {\n    deleteEvent() {\n      Store.mutations.deleteEvent(this.day.id, this.event.title);\n    },\n\n    editEvent() {\n      Store.mutations.editEvent(this.day.id, this.event.title);\n    },\n\n    updateEvent() {\n      Store.mutations.updateEvent(this.day.id, this.event.title, {\n        title: this.newEventTitle,\n        priority: this.newEventPriority\n      });\n    },\n\n    setNewEventTitle(event) {\n      this.newEventTitle = event.target.value;\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAqCA,OAAOA,KAAP,MAAkB,UAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,eADO;EAEbC,KAAK,EAAE;IACLC,KAAK,EAAEC,MADF;IAELC,GAAG,EAAED;EAFA,CAFM;;EAMbE,IAAI,GAAG;IACL,OAAO;MACLC,aAAa,EAAE,EADV;MAELC,gBAAgB,EAAE,KAAKL,KAAL,CAAWM;IAFxB,CAAP;EAID,CAXY;;EAYbC,QAAQ,EAAE;IACRC,mBAAmB,GAAG;MACpB,QAAQ,KAAKR,KAAL,CAAWM,QAAnB;QACE,KAAK,CAAL;UACE,OAAO,SAAP;;QACF,KAAK,CAAL;UACE,OAAO,QAAP;;QACF,KAAK,CAAC,CAAN;UACE,OAAO,MAAP;MANJ;;MAQA,OAAO,iBAAP;IACD,CAXO;;IAYRG,UAAU,GAAG;MACX,OAAO,WAAW,KAAKT,KAAL,CAAWU,KAA7B;IACD;;EAdO,CAZG;EA4BbC,OAAO,EAAE;IACPC,WAAW,GAAG;MACZf,KAAK,CAACgB,SAAN,CAAgBD,WAAhB,CAA4B,KAAKV,GAAL,CAASY,EAArC,EAAyC,KAAKd,KAAL,CAAWe,KAApD;IACD,CAHM;;IAIPC,SAAS,GAAG;MACVnB,KAAK,CAACgB,SAAN,CAAgBG,SAAhB,CAA0B,KAAKd,GAAL,CAASY,EAAnC,EAAuC,KAAKd,KAAL,CAAWe,KAAlD;IACD,CANM;;IAOPE,WAAW,GAAG;MACZpB,KAAK,CAACgB,SAAN,CAAgBI,WAAhB,CAA4B,KAAKf,GAAL,CAASY,EAArC,EAAyC,KAAKd,KAAL,CAAWe,KAApD,EAA2D;QACzDA,KAAK,EAAE,KAAKX,aAD6C;QAEzDE,QAAQ,EAAE,KAAKD;MAF0C,CAA3D;IAID,CAZM;;IAaPa,gBAAgB,CAAClB,KAAD,EAAQ;MACtB,KAAKI,aAAL,GAAqBJ,KAAK,CAACmB,MAAN,CAAaC,KAAlC;IACD;;EAfM;AA5BI,CAAf","names":["Store","name","props","event","Object","day","data","newEventTitle","newEventPriority","priority","computed","priorityDisplayName","alertColor","color","methods","deleteEvent","mutations","id","title","editEvent","updateEvent","setNewEventTitle","target","value"],"sourceRoot":"","sources":["/Users/isaak/Desktop/vuejs_udemy_deutsch/terminkalender/src/components/CalendarEvent.vue"],"sourcesContent":["<template>\n  <div class=\"alert text-center\" :class=\"alertColor\">\n    <template v-if=\"!event.edit\">\n      <div>\n        <slot name=\"eventPriority\" :priorityDisplayName=\"priorityDisplayName\">\n          <strong>{{ priorityDisplayName }}</strong>\n        </slot>\n      </div>\n\n      <slot :event=\"event\">\n        <div>{{ event.title }}</div>\n      </slot>\n\n      <div>\n        <i class=\"fas fa-edit me-2\" role=\"button\" @click=\"editEvent()\"></i>\n        <i class=\"far fa-trash-alt\" role=\"button\" @click=\"deleteEvent()\"></i>\n      </div>\n    </template>\n    <template v-else>\n      <input\n        type=\"text\"\n        class=\"form-control\"\n        :placeholder=\"event.title\"\n        @input=\"setNewEventTitle($event)\"\n      />\n      <select class=\"form-select mt-2\" v-model=\"newEventPriority\">\n        <option value=\"-1\">Hoch</option>\n        <option value=\"0\">Mittel</option>\n        <option value=\"1\">Tief</option>\n      </select>\n      <hr />\n      <i class=\"fas fa-check\" role=\"button\" @click=\"updateEvent()\"></i>\n    </template>\n  </div>\n</template>\n\n<script>\nimport Store from \"../store\";\n\nexport default {\n  name: \"CalendarEvent\",\n  props: {\n    event: Object,\n    day: Object,\n  },\n  data() {\n    return {\n      newEventTitle: \"\",\n      newEventPriority: this.event.priority,\n    };\n  },\n  computed: {\n    priorityDisplayName() {\n      switch (this.event.priority) {\n        case 1:\n          return \"Niedrig\";\n        case 0:\n          return \"Mittel\";\n        case -1:\n          return \"Hoch\";\n      }\n      return \"Unbekannte Prio\";\n    },\n    alertColor() {\n      return \"alert-\" + this.event.color;\n    },\n  },\n  methods: {\n    deleteEvent() {\n      Store.mutations.deleteEvent(this.day.id, this.event.title);\n    },\n    editEvent() {\n      Store.mutations.editEvent(this.day.id, this.event.title);\n    },\n    updateEvent() {\n      Store.mutations.updateEvent(this.day.id, this.event.title, {\n        title: this.newEventTitle,\n        priority: this.newEventPriority,\n      });\n    },\n    setNewEventTitle(event) {\n      this.newEventTitle = event.target.value;\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}