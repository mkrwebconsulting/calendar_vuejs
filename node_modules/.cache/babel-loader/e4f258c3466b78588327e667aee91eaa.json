{"ast":null,"code":"import CalendarEvent from \"./CalendarEvent\";\nimport Store from \"../store\";\nexport default {\n  name: \"CalendarDay\",\n  components: {\n    CalendarEvent\n  },\n  props: {\n    day: {\n      type: Object,\n      required: true,\n      default: function () {\n        return {\n          id: -1,\n          fullName: \"Fehlender Wochentag\",\n          events: []\n        };\n      },\n      validator: function (value) {\n        if (Object.keys(value).includes(\"id\")) {\n          return true;\n        }\n      }\n    }\n  },\n  computed: {\n    cardClasses() {\n      return this.day.id === Store.getters.activeDay().id ? [\"border-primary\"] : null;\n    },\n\n    cardHeaderClasses() {\n      return this.day.id === Store.getters.activeDay().id ? [\"bg-primary\", \"text-white\"] : null;\n    },\n\n    events() {\n      return Store.getters.events(this.day.id);\n    }\n\n  },\n  methods: {\n    setActiveDay() {\n      Store.mutations.setActiveDay(this.day.id);\n    }\n\n  }\n};","map":{"version":3,"mappings":"AAwCA,OAAOA,aAAP,MAA0B,iBAA1B;AACA,OAAOC,KAAP,MAAkB,UAAlB;AAEA,eAAe;EACbC,IAAI,EAAE,aADO;EAEbC,UAAU,EAAE;IACVH;EADU,CAFC;EAMbI,KAAK,EAAE;IACLC,GAAG,EAAE;MACHC,IAAI,EAAEC,MADH;MAEHC,QAAQ,EAAE,IAFP;MAGHC,OAAO,EAAE,YAAY;QACnB,OAAO;UACLC,EAAE,EAAE,CAAC,CADA;UAELC,QAAQ,EAAE,qBAFL;UAGLC,MAAM,EAAE;QAHH,CAAP;MAKD,CATE;MAUHC,SAAS,EAAE,UAAUC,KAAV,EAAiB;QAC1B,IAAIP,MAAM,CAACQ,IAAP,CAAYD,KAAZ,EAAmBE,QAAnB,CAA4B,IAA5B,CAAJ,EAAuC;UACrC,OAAO,IAAP;QACF;MACD;IAdE;EADA,CANM;EAyBbC,QAAQ,EAAE;IACRC,WAAW,GAAG;MACZ,OAAO,KAAKb,GAAL,CAASK,EAAT,KAAgBT,KAAK,CAACkB,OAAN,CAAcC,SAAd,GAA0BV,EAA1C,GACH,CAAC,gBAAD,CADG,GAEH,IAFJ;IAGD,CALO;;IAMRW,iBAAiB,GAAG;MAClB,OAAO,KAAKhB,GAAL,CAASK,EAAT,KAAgBT,KAAK,CAACkB,OAAN,CAAcC,SAAd,GAA0BV,EAA1C,GACH,CAAC,YAAD,EAAe,YAAf,CADG,GAEH,IAFJ;IAGD,CAVO;;IAWRE,MAAM,GAAG;MACP,OAAOX,KAAK,CAACkB,OAAN,CAAcP,MAAd,CAAqB,KAAKP,GAAL,CAASK,EAA9B,CAAP;IACD;;EAbO,CAzBG;EAwCbY,OAAO,EAAE;IACPC,YAAY,GAAG;MACbtB,KAAK,CAACuB,SAAN,CAAgBD,YAAhB,CAA6B,KAA