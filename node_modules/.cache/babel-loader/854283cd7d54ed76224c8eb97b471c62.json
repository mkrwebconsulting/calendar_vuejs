tton class=\"btn btn-primary\">Eintragen</button>\n          <button class=\"btn btn-danger\">Inhalt löschen</button>\n        </div>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport Store from \"../store\";\n\nexport default {\n    name: \"CalendarEntry\",\n    data() {\n      eventColor [\"primary\", \"success\", \"info\", \"warning\", \"danger\"];\n    },\n  computed: {\n    activeDayName() {\n      return Store.getters.activeDay().fullName;\n    },\n  },\n\n  methods: {\n    eventColorClasses(eventColor) {\n      return [\"alert-\" + eventColor];\n    },\n  },\n};\n</script>\n\n<style scoped></style>\n"]},"metadata":{},"sourceType":"module"}